        <name>getmember</name>
        <script><![CDATA[var getmember = Class.create();
getmember.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

	checkGroup: function(){

		var k=[];
		var userName = this.getParameter('userName');
	var grSysUser = new GlideRecord('sys_user_grmember');
		grSysUser.addQuery('group',userName);
		grSysUser.query();
	
	while (grSysUser.next()) {
		k.push(grSysUser.getDisplayValue('user.email'));
	}
		return k.toString();
	},
   
    type: 'getmember'
});]]></script>
